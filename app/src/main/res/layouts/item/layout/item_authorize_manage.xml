<?xml version="1.0" encoding="utf-8"?><!--    qmui_general_shadow_elevation-->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginStart="@dimen/dp_20"
    android:layout_marginEnd="@dimen/dp_20"
    android:layout_marginTop="@dimen/dp_10"
    android:background="@drawable/bg_radius6_white"
    android:orientation="vertical">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_70">

        <com.qmuiteam.qmui.widget.QMUIRadiusImageView
            android:id="@+id/ivHead"
            android:layout_width="@dimen/dp_50"
            android:layout_height="@dimen/dp_50"
            android:layout_marginStart="@dimen/dp_15"
            android:src="@mipmap/ic_default_head"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:qmui_corner_radius="@dimen/dp_25" />

        <TextView
            android:id="@+id/tvName"
            style="@style/JgTextView_MainColor"
            android:text="好大一棵树"
            app:layout_constraintTop_toTopOf="@id/ivHead"
            app:layout_constraintStart_toEndOf="@+id/ivHead"
            android:layout_marginStart="@dimen/dp_10"
            android:textSize="@dimen/sp_15"/>

        <TextView
            android:layout_height="@dimen/dp_20"
            android:gravity="center"
            android:layout_width="wrap_content"
            android:text="儿子"
            app:layout_constraintTop_toBottomOf="@id/tvName"
            app:layout_constraintStart_toEndOf="@+id/ivHead"
            android:background="@drawable/bg_radius10_9cb3ff"
            android:paddingStart="@dimen/dp_8"
            android:paddingEnd="@dimen/dp_8"
            android:layout_marginStart="@dimen/dp_10"
            android:layout_marginTop="@dimen/dp_5"
            android:textColor="@color/white"
            android:textSize="@dimen/sp_13"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <View style="@style/dividingLine_Horizontal_Margin15" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_47"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingStart="@dimen/dp_15">

        <TextView
            style="@style/JgTextView_ViceColor"
            android:layout_weight="1"
            android:text="指纹权限" />

        <TextView
            style="@style/JgItemTextView_Next"
            android:layout_width="wrap_content"
            android:hint="@string/str_noopen"
            android:textColorHint="@color/c_cccccc" />

    </LinearLayout>

    <View style="@style/dividingLine_Horizontal_Margin15" />


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_47"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingStart="@dimen/dp_15">

        <TextView
            style="@style/JgTextView_ViceColor"
            android:layout_weight="1"
            android:text="人脸权限" />

        <TextView
            style="@style/JgItemTextView_Next"
            android:layout_width="wrap_content"
            android:hint="@string/str_noopen"
            android:textColorHint="@color/c_cccccc" />

    </LinearLayout>

    <View style="@style/dividingLine_Horizontal_Margin15" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_47"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingStart="@dimen/dp_15"
        android:paddingEnd="@dimen/dp_15">

        <TextView
            style="@style/JgTextView_ViceColor"
            android:layout_weight="1"
            android:text="监控权限" />

        <Switch
            android:id="@+id/swMsgHint"
            style="@style/JgSwitchStyle"
            android:checked="true"
            tools:ignore="UseSwitchCompatOrMaterialXml" />

    </LinearLayout>

    <View style="@style/dividingLine_Horizontal_Margin15" />


</LinearLayout>



