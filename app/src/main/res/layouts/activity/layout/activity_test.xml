<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.fengdi.baseproject.mvp.ui.widget.MyToolBar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:title="测试界面" />

    <Button
        android:id="@+id/button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="图片选择器" />

    <Button
        android:id="@+id/button2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="扫描二维码" />

    <Button
        android:id="@+id/button3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="圆角背景" />

    <Button
        android:id="@+id/button4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="对话框" />

    <Button
        android:id="@+id/button5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="成功对话框" />

    <Button
        android:id="@+id/button6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="微信支付" />

    <Button
        android:id="@+id/button7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="微信登陆" />


    <com.qmuiteam.qmui.widget.QMUIRadiusImageView
        android:layout_width="@dimen/dp_50"
        android:layout_height="@dimen/dp_50"
        android:layout_gravity="center"
        android:layout_marginTop="@dimen/dp_20"
        android:src="@mipmap/ic_default_head"
        app:qmui_corner_radius="@dimen/dp_25" />

    <!--    qmui_general_shadow_elevation-->
    <com.qmuiteam.qmui.layout.QMUILinearLayout
        android:id="@+id/qLinTest"
        android:layout_width="260dp"
        android:layout_height="260dp"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="40dp"
        android:background="@color/qmui_config_color_white"
        android:gravity="center"
        android:orientation="vertical"
        app:qmui_borderColor="?attr/qmui_skin_support_color_separator"
        app:qmui_outerNormalColor="?attr/qmui_skin_support_color_background"
        app:qmui_shadowAlpha="0.3"
        app:qmui_showBorderOnlyBeforeL="true">

    </com.qmuiteam.qmui.layout.QMUILinearLayout>

</LinearLayout>